<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="orderDetail">

	<!-- 生成订单明细 -->
    <insert id="insertOrderDetail">
      insert into `orderDetail`(orderId,goodId,goodsNumber) values(#{orderId},#{goodId},#{goodsNumber})
    </insert>
    <!-- 删除订单明细 -->
   <delete id="delOrderD" parameterType="String">
      delete from `orderDetail`  where orderId=#{id}
   </delete>
   
</mapper>